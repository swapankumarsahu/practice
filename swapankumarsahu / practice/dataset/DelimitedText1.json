{
	"name": "DelimitedText1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SourceContainerName": {
				"type": "string",
				"defaultValue": "@{item().SourceContainerName}"
			},
			"SourceFolderName": {
				"type": "string",
				"defaultValue": "@{item().SourceFolderName}"
			},
			"SourceFileLoacation": {
				"type": "string",
				"defaultValue": "@{item().SourceFileLoacation}"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().SourceFolderName,'/', dataset().SourceFileLoacation)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().SourceContainerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}