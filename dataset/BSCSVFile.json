{
	"name": "BSCSVFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ABSConnection",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "@{item().Query}.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "Test",
					"type": "Expression"
				},
				"container": "filestore"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}